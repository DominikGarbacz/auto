    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/ParkingLot"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
