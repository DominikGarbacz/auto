    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/CityStreet">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Trafficway"/>
        <rdfs:label xml:lang="en">city street</rdfs:label>
        <skos:definition xml:lang="en">A trafficway within a city trafficway system that is not an Interstate highway, other U.S. route numbered highway, other state route numbered highway, or county road.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.8</fibo-fnd-utl-av:adaptedFrom>
