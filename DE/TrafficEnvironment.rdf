        <rdfs:label xml:lang="en">channelized intersection</rdfs:label>
        <skos:definition xml:lang="en">An at-grade intersection in which traffic is diverted into definite paths by raised or painted traffic islands.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.13</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


