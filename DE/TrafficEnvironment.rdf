        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:label xml:lang="en">crosswalk</rdfs:label>
        <skos:definition xml:lang="en">(1) part of a roadway at an intersection included within the connections of the lateral lines of the sidewalks on opposite sides of the roadway measured from the curbs or, in the absence of curbs, from the edges of the traversable roadway, or (2) portion of a roadway at an intersection or elsewhere distinctly indicated for pedestrian crossing by lines or other markings on the surface of the roadway</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Crosswalk"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">(1) part of a roadway at an intersection included within the connections of the lateral lines of the sidewalks on opposite sides of the roadway measured from the curbs or, in the absence of curbs, from the edges of the traversable roadway, or (2) portion of a roadway at an intersection or elsewhere distinctly indicated for pedestrian crossing by lines or other markings on the surface of the roadway</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.21</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/CrosswalkRoadSegment -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/CrosswalkRoadSegment">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSegment"/>
        <rdfs:label xml:lang="en">crosswalk road segment</rdfs:label>
        <skos:definition xml:lang="en">road segment which contains a crosswalk</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/CurbReturn -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/CurbReturn">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:label xml:lang="en">curb return</rdfs:label>
        <skos:definition xml:lang="en">curved section of curb used at intersections in joining straight sections of curb</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/CurbReturn"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">curved section of curb used at intersections in joining straight sections of curb</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.20</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/DirectionalRoadSign -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/DirectionalRoadSign">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSign"/>
        <rdfs:label xml:lang="en">directional road sign</rdfs:label>
        <skos:definition xml:lang="en">sign installed on public land that advertises the direction to be taken to a service, tourist attraction or a townsite/locality</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/DirectionalRoadSign"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">sign installed on public land that advertises the direction to be taken to a service, tourist attraction or a townsite/locality</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>https://www.lawinsider.com/dictionary/directional-signage</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Driveway -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Driveway">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Trafficway"/>
        <rdfs:comment xml:lang="en">A driveway is outside the trafficway and is typically not provided an official identification name or number.</rdfs:comment>
        <rdfs:label xml:lang="en">driveway</rdfs:label>
        <skos:definition xml:lang="en">A private way which provides vehicular access to the public from a trafficway to property, parking or loading areas outside the boundaries of the trafficway, but is considered to be not open to the public for transportation purposes as a trafficway.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.9.1</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/DrivewayAccess -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/DrivewayAccess">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:label xml:lang="en">driveway access</rdfs:label>
        <skos:definition xml:lang="en">portion of the trafficway at the end of a driveway (See 2.5.9.1), providing access to property adjacent to a trafficway</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/DrivewayAccess"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">portion of the trafficway at the end of a driveway (See 2.5.9.1), providing access to property adjacent to a trafficway</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.9</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/EducationalInstitute -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/EducationalInstitute">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Building"/>
        <rdfs:label xml:lang="en">educational institute</rdfs:label>
        <skos:definition xml:lang="en">institute that provides education as its main purpose</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/EndLane -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/EndLane">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/LaneSegment"/>
        <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/StartLane"/>
        <rdfs:label xml:lang="en">end lane</rdfs:label>
        <skos:definition xml:lang="en">lane located at the end of a path</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/EndNode -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/EndNode">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Node"/>
        <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/StartNode"/>
        <rdfs:label xml:lang="en">end node</rdfs:label>
        <skos:definition xml:lang="en">end GPS position of a path</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FastFood -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FastFood">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Restaurant"/>
        <rdfs:label xml:lang="en">fast food</rdfs:label>
        <skos:definition xml:lang="en">restaurant specializing in food that can be prepared and served quickly</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FastFood"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">restaurant specializing in food that can be prepared and served quickly</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>https://www.merriam-webster.com/dictionary/fast-food</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FireStation -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FireStation">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Building"/>
        <rdfs:label xml:lang="en">fire station</rdfs:label>
        <skos:definition xml:lang="en">building housing fire apparatus and usually firefighters</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FireStation"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">building housing fire apparatus and usually firefighters</owl:annotatedTarget>
        <sm:directSource>https://www.merriam-webster.com/dictionary/fire%20station</sm:directSource>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Forest -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Forest">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/NaturalPlace"/>
        <rdfs:label xml:lang="en">forest</rdfs:label>
        <skos:definition xml:lang="en">dense growth of trees and underbrush covering a large tract</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Forest"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">dense growth of trees and underbrush covering a large tract</owl:annotatedTarget>
        <sm:directSource>https://www.merriam-webster.com/dictionary/forest</sm:directSource>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FrontageRoad -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/FrontageRoad">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Trafficway"/>
        <rdfs:comment xml:lang="en">The frontage road may be within the same trafficway as the main roadway or in a separate trafficway.</rdfs:comment>
        <rdfs:label xml:lang="en">frontage road</rdfs:label>
        <skos:definition xml:lang="en">A roadway generally paralleling an expressway, freeway, parkway or through street so designed as to intercept, collect and distribute traffic desiring to cross, enter or leave such facility and to furnish access to property which otherwise would be isolated as a result of controlled-access features.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.18</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Fully-controlledAccessHighway -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Fully-controlledAccessHighway">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Trafficway"/>
        <rdfs:label xml:lang="en">fully-controlled access highway</rdfs:label>
        <skos:definition xml:lang="en">A trafficway on which preference is given to through traffic by permitting access only from other trafficways and by providing grade separations at all crossing trafficways.</skos:definition>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.15</fibo-fnd-utl-av:adaptedFrom>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/GasStation -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/GasStation">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Infrastructure"/>
        <rdfs:label xml:lang="en">gas station</rdfs:label>
        <skos:definition xml:lang="en">retail station for servicing motor vehicles, especially with gasoline and oil</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/GasStation"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">retail station for servicing motor vehicles, especially with gasoline and oil</owl:annotatedTarget>
        <sm:directSource>https://www.merriam-webster.com/dictionary/gas%20station</sm:directSource>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Gore -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Gore">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:comment xml:lang="en">The area is bounded on two sides by the edges of the roadways, which join at the point of divergence or convergence. The direction of traffic shall be the same on both sides of these roadways. The area includes shoulders or marked pavement, if any, between the roadways. The third side is 60 meters (approximately 200 feet) from the point of divergence or convergence or, if any other road is within 70 meters (230 feet) of that point, a line 10 meters (33 feet) from the nearest edge of such road.</rdfs:comment>
        <rdfs:label xml:lang="en">gore</rdfs:label>
        <skos:definition xml:lang="en">area of land where two roadways diverge or converge</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Gore"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">area of land where two roadways diverge or converge</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.19</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/GradeSeparation -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/GradeSeparation">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Intersection"/>
        <rdfs:label xml:lang="en">grade separation</rdfs:label>
        <skos:definition xml:lang="en">crossing at different levels of two trafficways, or a trafficway and a railway</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/GradeSeparation"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">crossing at different levels of two trafficways, or a trafficway and a railway</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.14</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Highway -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Highway">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadType"/>
        <rdfs:label xml:lang="en">highway</rdfs:label>
        <skos:definition xml:lang="en">road that is part of the strategic traffic network for automobiles across the country and connects areas of political/economic importance or areas that are critical to the nation&apos;s interests</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Hospital -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Hospital">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Building"/>
        <rdfs:label xml:lang="en">hospital</rdfs:label>
        <skos:definition xml:lang="en">institution where the sick or injured are given medical or surgical care</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Hospital"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">institution where the sick or injured are given medical or surgical care</owl:annotatedTarget>
        <sm:directSource>https://www.merriam-webster.com/dictionary/hospital</sm:directSource>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Infrastructure -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Infrastructure">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/MaterialEntity"/>
        <rdfs:label xml:lang="en">infrastructure</rdfs:label>
        <skos:definition xml:lang="en">basic installations and facilities on which the continuance and growth of a community, state, etc. depend, such as roads, schools, power plants, transportation and communication systems, etc.</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Infrastructure"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">basic installations and facilities on which the continuance and growth of a community, state, etc. depend, such as roads, schools, power plants, transportation and communication systems, etc.</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>https://www.collinsdictionary.com/dictionary/english/infrastructure</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InstructionRoadSign -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InstructionRoadSign">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSign"/>
        <rdfs:label xml:lang="en">instruction road sign</rdfs:label>
        <skos:definition xml:lang="en">sign which does not contain advertising or promotional matter but relates solely to traffic or pedestrian control, warnings or hours of operation</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InstructionRoadSign"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">sign which does not contain advertising or promotional matter but relates solely to traffic or pedestrian control, warnings or hours of operation</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>https://www.lawinsider.com/dictionary/instructional-sign</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InstructionSurfaceSign -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InstructionSurfaceSign">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSurfaceMarking"/>
        <rdfs:label xml:lang="en">instruction surface sign</rdfs:label>
        <skos:definition xml:lang="en">sign which does not contain advertising or promotional matter but relates solely to traffic or pedestrian control, warnings or hours of operation, that is located on the road surface</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Interchange -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Interchange">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:label xml:lang="en">interchange</rdfs:label>
        <skos:definition xml:lang="en">system of interconnecting roadways in conjunction with one or more grade separations, providing for the movement of traffic between two or more roadways on different levels</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Interchange"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">system of interconnecting roadways in conjunction with one or more grade separations, providing for the movement of traffic between two or more roadways on different levels</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.16</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Intersection -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Intersection">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Junction"/>
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:comment xml:lang="en">Where the distance along a roadway between two areas meeting these criteria is less than 10 meters (33 feet), the two areas and the roadway connecting them are considered to be parts of a single intersection.</rdfs:comment>
        <rdfs:label xml:lang="en">intersection</rdfs:label>
        <skos:definition xml:lang="en">area which (1) contains a crossing or connection of two or more roadways not classified as driveway access and (2) is embraced within the prolongation of the lateral curb lines or, if none, the lateral boundary lines of the roadways</skos:definition>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Intersection"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2004/02/skos/core#definition"/>
        <owl:annotatedTarget xml:lang="en">area which (1) contains a crossing or connection of two or more roadways not classified as driveway access and (2) is embraced within the prolongation of the lateral curb lines or, if none, the lateral boundary lines of the roadways</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>ANSI D.16-2017, section 2.5.10</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/IntersectionSegment -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/IntersectionSegment">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/PathSegment"/>
        <owl:disjointWith rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/LaneSegment"/>
        <rdfs:label xml:lang="en">intersection segment</rdfs:label>
        <skos:definition xml:lang="en">path segment that contains an intersection of two or more lanes</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InterstateHighway -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/InterstateHighway">
        <rdfs:subClassOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Trafficway"/>
        <rdfs:label xml:lang="en">interstate highway</rdfs:label>
        <skos:definition xml:lang="en">A trafficway on the Interstate System.</skos:definition>
