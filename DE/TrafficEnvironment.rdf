<?xml version="1.0"?>
<rdf:RDF xmlns="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/"
     xml:base="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:auto-ev="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/"
     xmlns:auto-mo="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/"
     xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/">
    <owl:Ontology rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/">
        <owl:imports rdf:resource="https://spec.edmcouncil.org/auto/ontology/MO/MiddleOntology/"/>
        <owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
        <dct:abstract xml:lang="en">A sophisticated machine understandable map is required for autonomous vehicles to perceive driving environments. Therefore, we construct a map ontology.</dct:abstract>
        <dct:license rdf:resource="https://creativecommons.org/licenses/by-nc-sa/4.0/"/>
        <sm:contentLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.w3.org/standards/techs/owl#w3c_all</sm:contentLanguage>
        <sm:dependsOn rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://spec.edmcouncil.org/auto/ontology/EC/</sm:dependsOn>
        <sm:fileAbbreviation>auto-de-map</sm:fileAbbreviation>
        <sm:filename>Map.rdf</sm:filename>
        <rdfs:comment xml:lang="en">The traffic environment ontology (TE) provides a model for traffic infrastructure. 

			It contains many classes and properties extracted from: Map Ontology created by Lihua Zhao and Yutaka Sasaki at the Computational Intelligence Laboratory, Toyota Technological Institute, Nagoya, Japan (see https://www.toyota-ti.ac.jp/Lab/Denshi/COIN/Ontology/TTICore-0.03/) and published by the Toyota Technological Institute Smart Vehicle Research Project.</rdfs:comment>
        <rdfs:label xml:lang="en">Traffic environment ontology (TE)</rdfs:label>
        <fibo-fnd-utl-av:hasMaturityLevel rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/Provisional"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/publisher -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/publisher"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/goStraightTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/goStraightTo">
        <rdfs:isDefinedBy xml:lang="en">relates a vehicle to a road part straight of him, onto which it should go when changing lanes</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">go straight to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasCar -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasCar">
        <rdfs:label xml:lang="en">has car</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasIntersection -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasIntersection">
        <rdfs:subPropertyOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasJunction"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadType"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Intersection"/>
        <rdfs:isDefinedBy xml:lang="en">relates a road type to an intersection</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has intersection</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasJunction -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasJunction">
        <rdfs:isDefinedBy xml:lang="en">relates a road type to a junction</rdfs:isDefinedBy>
        <rdfs:label>has junction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasLane -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasLane">
        <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isLaneOf"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSegment"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:isDefinedBy xml:lang="en">relates a road segment to a lane it contains</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has lane</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasRoadSegment -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/hasRoadSegment">
        <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isRoadSegmentOf"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSegment"/>
        <rdfs:isDefinedBy xml:lang="en">relates a road part to a road segment it contains</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">has road segment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isConnectedTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isConnectedTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:isDefinedBy xml:lang="en">relates a road to a road connected to it</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is connected to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isLaneOf -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isLaneOf">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSegment"/>
        <rdfs:isDefinedBy xml:lang="en">relates a lane to a road segment which contains said lane</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is lane of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isLeftOf -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isLeftOf">
        <owl:inverseOf rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isRightOf"/>
        <rdfs:label xml:lang="en">is left of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isNextTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isNextTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:range rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:isDefinedBy xml:lang="en">relates a lane to a lane next to it</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is next to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isOn -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isOn">
        <rdfs:label xml:lang="en">is on</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isRightOf -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isRightOf">
        <rdfs:label xml:lang="en">is right of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isRoadSegmentOf -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isRoadSegmentOf">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadSegment"/>
        <rdfs:isDefinedBy xml:lang="en">relates a road segment to its road part</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">is road segment of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/nearTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/nearTo">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:label xml:lang="en">near to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/relatedTrafficLight -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/relatedTrafficLight">
        <rdfs:isDefinedBy xml:lang="en">links a driver to the traffic lights they should observe on a lane</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">related traffic light</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/turnLeftTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/turnLeftTo">
        <rdfs:isDefinedBy xml:lang="en">relates a vehicle to a road part left of him, onto which it should go when changing lanes</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">turn left to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/turnRightTo -->

    <owl:ObjectProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/turnRightTo">
        <rdfs:isDefinedBy xml:lang="en">relates a vehicle to a road part right of him, onto which it should go when changing lanes</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">turn right to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/address -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/address">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Infrastructure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy xml:lang="en">relates an infrastructure to its address</rdfs:isDefinedBy>
        <rdfs:label>address</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/adminArea -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/adminArea">
        <rdfs:isDefinedBy xml:lang="en">relates a node, an infrastructure or a natural place to its administrative area</rdfs:isDefinedBy>
        <rdfs:label>admin area</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/allowLaneChange -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/allowLaneChange">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:isDefinedBy xml:lang="en">indicates whether a vehicle can change lanes</rdfs:isDefinedBy>
        <rdfs:label>allow lane change</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/allowedDir -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/allowedDir">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:label>allowed direction</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/alt -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/alt">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy xml:lang="en">relates a vehicle or a node to its altitude expressed in meters</rdfs:isDefinedBy>
        <rdfs:label>altitude</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/angle -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/angle">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/TrafficSignal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/boundCoordinate -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/boundCoordinate">
        <rdfs:label>bound coordinate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/boundPos -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/boundPos">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:isDefinedBy xml:lang="en">relates a junction or a road segment with its boundary GPS point</rdfs:isDefinedBy>
        <rdfs:label>bound position</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/brand -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/brand">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy xml:lang="en">relates a material entity to its brand</rdfs:isDefinedBy>
        <rdfs:label>brand</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/curve_id -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/curve_id">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>curve ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/drivingDir -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/drivingDir">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:isDefinedBy xml:lang="en">relates a lane to its driving direction</rdfs:isDefinedBy>
        <rdfs:label>driving direction</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/enterPos -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/enterPos">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:label>enter position</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/exitPos -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/exitPos">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Lane"/>
        <rdfs:label>exit position</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isEmpty -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/isEmpty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/RoadPart"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:isDefinedBy xml:lang="en">indicates whether a given road part is empty</rdfs:isDefinedBy>
        <rdfs:label>is empty</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/lat -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/lat">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy xml:lang="en">relates a vehicle or a node to its latitude</rdfs:isDefinedBy>
        <rdfs:label>latitude</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/loc_x -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/loc_x">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy xml:lang="en">relates a node to its x coordinate</rdfs:isDefinedBy>
        <rdfs:label>location x</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/loc_y -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/loc_y">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy xml:lang="en">relates a node to its y coordinate</rdfs:isDefinedBy>
        <rdfs:label>location y</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/loc_z -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/loc_z">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy xml:lang="en">relates a node to its z coordinate</rdfs:isDefinedBy>
        <rdfs:label>location z</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/lon -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/lon">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:isDefinedBy xml:lang="en">relates a vehicle or a node to its longitude</rdfs:isDefinedBy>
        <rdfs:label>longitude</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/name -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/name">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:isDefinedBy xml:lang="en">relates something to its name</rdfs:isDefinedBy>
        <rdfs:label>name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/opening_hours -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/opening_hours">
        <rdfs:isDefinedBy xml:lang="en">relates an infrastructure or a natural place to the hours during which it is open</rdfs:isDefinedBy>
        <rdfs:label>opening hours</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/operator -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/operator">
        <rdfs:domain rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Infrastructure"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>operator</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/orientation -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/orientation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:label>orientation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/osm_node_id -->

    <owl:DatatypeProperty rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/osm_node_id">
