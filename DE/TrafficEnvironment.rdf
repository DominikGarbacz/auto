        <rdfs:label xml:lang="en">bicycle trail</rdfs:label>
        <skos:definition xml:lang="en">A bikeway reserved for pedalcycles and separated from roadways by open space or barriers.</skos:definition>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Bikeway -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/Bikeway">
