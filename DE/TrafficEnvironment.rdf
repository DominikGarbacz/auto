    <!-- https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/BicycleLane -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/TrafficEnvironment/BicycleLane">
