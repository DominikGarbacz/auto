        <rdfs:label xml:lang="en">forward collision warning</rdfs:label>
    </owl:Class>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/CarControl/Go -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/CarControl/Go">
