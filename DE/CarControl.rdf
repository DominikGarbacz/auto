        <rdfs:label xml:lang="en">enhancing conspicuity</rdfs:label>
        <fibo-fnd-utl-av:explanatoryNote xml:lang="en">Enhancing conspicuity via lighting, signaling and gesturing, etc.</fibo-fnd-utl-av:explanatoryNote>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://spec.edmcouncil.org/auto/ontology/DE/CarControl/EnhancingConspicuity"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#isDefinedBy"/>
        <owl:annotatedTarget xml:lang="en">DDT subtask comprising activities which make the vehicle more visible and distinguishable from its surroundings, such as by lightning, sounding the horn, gesturing etc.</owl:annotatedTarget>
        <fibo-fnd-utl-av:adaptedFrom>J3016 JUN2018, section 3.10</fibo-fnd-utl-av:adaptedFrom>
    </owl:Axiom>
    


    <!-- https://spec.edmcouncil.org/auto/ontology/DE/CarControl/ForwardCollisionWarning -->

    <owl:Class rdf:about="https://spec.edmcouncil.org/auto/ontology/DE/CarControl/ForwardCollisionWarning">
